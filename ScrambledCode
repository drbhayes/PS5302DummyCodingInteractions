plot1 <- ggplot(PaDSData, aes(x = PaDS_total, y = PHQ_Total)) +
  geom_point() +
  geom_smooth(se = FALSE, method = "lm") +
  theme_classic()

summary(PaDSData)

geom_boxplot() +
  ggplot(PaDSData, aes(x = gender_bin, y = PaDS_total)) +
  theme_classic() +
  labs(title = "Persecution by Gender", y = "Persecution", x = "Gender")

RegressionA <- lm(PaDS_total ~ age + gender_bin + PHQ_Total + AUS_total + DUS_total, data = PaDSData)

cor.test(PaDSData$PaDS_total, PaDSData$AUS_Total, method = "pearson")

PaDSData$gender_bin <- as.factor(PaDSData$gender_bin)

library(gridExtra)

RegressionB <- lm(PaDS_total ~ age + gender_bin*AUS_total + gender_bin*DUS_total, data = PaDSData)

PaDSData$PaDS_total <- as.numeric(PaDSData$PaDS_total)

cor.test(female$PaDS_total, female$DUS_total, method = "pearson")

